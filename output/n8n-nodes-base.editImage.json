  "configuration_logic": {
    "required_sequence": [
      "operation",
      "dataPropertyName",
      "operations",
      "backgroundColor",
      "width",
      "height",
      "primitive",
      "color",
      "startPositionX",
      "startPositionY",
      "endPositionX",
      "endPositionY",
      "cornerRadius",
      "text",
      "fontSize",
      "fontColor",
      "positionX",
      "positionY",
      "lineLength",
      "blur",
      "sigma",
      "borderWidth",
      "borderHeight",
      "borderColor",
      "dataPropertyNameComposite",
      "operator",
      "positionX",
      "positionY",
      "width",
      "height",
      "positionX",
      "positionY",
      "width",
      "height",
      "resizeOption",
      "rotate",
      "backgroundColor",
      "degreesX",
      "degreesY",
      "color",
      "options"
    ],
    "conditional_rules": [
      "IF operation == 'multiStep' THEN SHOW parameter 'operations'",
      "IF operation == 'create' THEN SHOW parameter 'backgroundColor'",
      "IF operation == 'create' THEN SHOW parameter 'width'",
      "IF operation == 'create' THEN SHOW parameter 'height'",
      "IF operation == 'draw' THEN SHOW parameter 'primitive'",
      "IF operation == 'draw' THEN SHOW parameter 'color'",
      "IF operation == 'draw' AND primitive IN [\"circle\", \"line\", \"rectangle\"] THEN SHOW parameter 'startPositionX'",
      "IF operation == 'draw' AND primitive IN [\"circle\", \"line\", \"rectangle\"] THEN SHOW parameter 'startPositionY'",
      "IF operation == 'draw' AND primitive IN [\"circle\", \"line\", \"rectangle\"] THEN SHOW parameter 'endPositionX'",
      "IF operation == 'draw' AND primitive IN [\"circle\", \"line\", \"rectangle\"] THEN SHOW parameter 'endPositionY'",
      "IF operation == 'draw' AND primitive == 'rectangle' THEN SHOW parameter 'cornerRadius'",
      "IF operation == 'text' THEN SHOW parameter 'text'",
      "IF operation == 'text' THEN SHOW parameter 'fontSize'",
      "IF operation == 'text' THEN SHOW parameter 'fontColor'",
      "IF operation == 'text' THEN SHOW parameter 'positionX'",
      "IF operation == 'text' THEN SHOW parameter 'positionY'",
      "IF operation == 'text' THEN SHOW parameter 'lineLength'",
      "IF operation == 'blur' THEN SHOW parameter 'blur'",
      "IF operation == 'blur' THEN SHOW parameter 'sigma'",
      "IF operation == 'border' THEN SHOW parameter 'borderWidth'",
      "IF operation == 'border' THEN SHOW parameter 'borderHeight'",
      "IF operation == 'border' THEN SHOW parameter 'borderColor'",
      "IF operation == 'composite' THEN SHOW parameter 'dataPropertyNameComposite'",
      "IF operation == 'composite' THEN SHOW parameter 'operator'",
      "IF operation == 'composite' THEN SHOW parameter 'positionX'",
      "IF operation == 'composite' THEN SHOW parameter 'positionY'",
      "IF operation == 'crop' THEN SHOW parameter 'width'",
      "IF operation == 'crop' THEN SHOW parameter 'height'",
      "IF operation == 'crop' THEN SHOW parameter 'positionX'",
      "IF operation == 'crop' THEN SHOW parameter 'positionY'",
      "IF operation == 'resize' THEN SHOW parameter 'width'",
      "IF operation == 'resize' THEN SHOW parameter 'height'",
      "IF operation == 'resize' THEN SHOW parameter 'resizeOption'",
      "IF operation == 'rotate' THEN SHOW parameter 'rotate'",
      "IF operation == 'rotate' THEN SHOW parameter 'backgroundColor'",
      "IF operation == 'shear' THEN SHOW parameter 'degreesX'",
      "IF operation == 'shear' THEN SHOW parameter 'degreesY'",
      "IF operation == 'transparent' THEN SHOW parameter 'color'",
      "IF operation == 'text' THEN SHOW parameter 'font'",
      "IF operation == 'information' THEN HIDE parameter 'options'",
      "IF /operation == 'text' THEN SHOW parameter 'font'",
      "IF format IN [\"jpeg\", \"png\", \"tiff\"] THEN SHOW parameter 'quality'"
    ]
  }
