  "configuration_logic": {
    "required_sequence": [
      "oldVersionNotice",
      "authentication",
      "authentication",
      "resource",
      "operation",
      "operation",
      "fileId",
      "fileId",
      "binaryPropertyName",
      "options",
      "useQueryString",
      "queryString",
      "limit",
      "queryFilters",
      "permissionsUi",
      "binaryData",
      "fileContent",
      "binaryPropertyName",
      "updateFields",
      "options",
      "name",
      "resolveData",
      "parents",
      "name",
      "options",
      "operation",
      "driveId",
      "name",
      "options",
      "options",
      "returnAll",
      "limit",
      "options",
      "options",
      "options"
    ],
    "conditional_rules": [
      "IF @version == 1 THEN SHOW parameter 'authentication'",
      "IF @version == 2 THEN SHOW parameter 'authentication'",
      "IF resource == 'file' THEN SHOW parameter 'operation'",
      "IF resource == 'folder' THEN SHOW parameter 'operation'",
      "IF operation IN [\"download\", \"copy\", \"update\", \"delete\", \"share\"] AND resource == 'file' THEN SHOW parameter 'fileId'",
      "IF operation IN [\"delete\", \"share\"] AND resource == 'folder' THEN SHOW parameter 'fileId'",
      "IF operation == 'download' AND resource == 'file' THEN SHOW parameter 'binaryPropertyName'",
      "IF operation == 'download' AND resource == 'file' THEN SHOW parameter 'options'",
      "IF operation == 'list' AND resource == 'file' THEN SHOW parameter 'useQueryString'",
      "IF operation == 'list' AND useQueryString == True AND resource == 'file' THEN SHOW parameter 'queryString'",
      "IF operation == 'list' AND resource == 'file' THEN SHOW parameter 'limit'",
      "IF operation == 'list' AND useQueryString == False AND resource == 'file' THEN SHOW parameter 'queryFilters'",
      "IF mimeType == 'custom' THEN SHOW parameter 'customMimeType'",
      "IF resource IN [\"file\", \"folder\"] AND operation == 'share' THEN SHOW parameter 'permissionsUi'",
      "IF type IN [\"user\", \"group\"] THEN SHOW parameter 'emailAddress'",
      "IF type == 'domain' THEN SHOW parameter 'domain'",
      "IF type IN [\"domain\", \"anyone\"] THEN SHOW parameter 'allowFileDiscovery'",
