        {
          "displayName": "Headers",
          "name": "headers",
          "placeholder": "Add Header",
          "description": "Headers to add",
          "type": "fixedCollection",
          "typeOptions": {
            "multipleValues": true
          },
          "default": {},
          "options": [
            {
              "name": "header",
              "displayName": "Header",
              "values": [
                {
                  "displayName": "Key",
                  "name": "key",
                  "type": "string",
                  "default": ""
                },
                {
                  "displayName": "Value",
                  "name": "value",
                  "type": "string",
                  "default": ""
                }
              ]
            }
          ]
        }
      ],
      "ai_hint": "此参数为 fixedCollection，包含嵌套字段，不要扁平化。"
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "operation",
      "operation",
      "deleteMessage",
      "mode",
      "queue",
      "exchange",
      "exchangeType",
      "routingKey",
      "sendInputData",
      "message",
      "options"
    ],
    "conditional_rules": [
      "IF @version == 1 THEN SHOW parameter 'operation'",
      "IF @version == 1.1 THEN SHOW parameter 'operation'",
      "IF operation == 'deleteMessage' THEN SHOW parameter 'deleteMessage'",
      "IF operation == 'deleteMessage' THEN HIDE parameter 'mode'",
      "IF mode == 'queue' THEN SHOW parameter 'queue'",
      "IF operation == 'deleteMessage' THEN HIDE parameter 'queue'",
      "IF mode == 'exchange' THEN SHOW parameter 'exchange'",
      "IF mode == 'exchange' THEN SHOW parameter 'exchangeType'",
      "IF mode == 'exchange' THEN SHOW parameter 'routingKey'",
      "IF operation == 'sendMessage' THEN SHOW parameter 'sendInputData'",
      "IF sendInputData == False THEN SHOW parameter 'message'",
      "IF operation == 'sendMessage' THEN SHOW parameter 'options'",
      "IF /mode == 'exchange' THEN SHOW parameter 'alternateExchange'",
      "IF /mode == 'queue' THEN SHOW parameter 'exclusive'"
    ]
  }
}