        {
          "displayName": "Durable",
          "name": "durable",
          "type": "boolean",
          "default": true,
          "description": "Whether the queue will survive broker restarts"
        },
        {
          "displayName": "Exclusive",
          "name": "exclusive",
          "type": "boolean",
          "displayOptions": {
            "show": {
              "/mode": [
                "queue"
              ]
            }
