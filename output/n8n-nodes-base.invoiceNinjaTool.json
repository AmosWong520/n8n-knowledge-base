      "IF operation == 'get' AND resource == 'client' THEN SHOW parameter 'options'",
      "IF resource == 'client' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'client' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getAll' AND resource == 'client' THEN SHOW parameter 'options'",
      "IF resource == 'invoice' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'invoice' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'invoice' AND operation == 'create' THEN SHOW parameter 'invoiceItemsUi'",
      "IF resource == 'invoice' AND operation == 'delete' THEN SHOW parameter 'invoiceId'",
      "IF resource == 'invoice' AND operation == 'email' THEN SHOW parameter 'invoiceId'",
      "IF resource == 'invoice' AND operation == 'get' THEN SHOW parameter 'invoiceId'",
      "IF operation == 'get' AND resource == 'invoice' THEN SHOW parameter 'options'",
      "IF resource == 'invoice' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'invoice' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getAll' AND resource == 'invoice' THEN SHOW parameter 'options'",
      "IF resource == 'task' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'task' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'task' AND operation == 'create' THEN SHOW parameter 'timeLogsUi'",
      "IF resource == 'task' AND operation == 'delete' THEN SHOW parameter 'taskId'",
