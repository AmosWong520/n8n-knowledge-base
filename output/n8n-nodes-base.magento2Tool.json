    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF resource == 'customer' THEN SHOW parameter 'operation'",
      "IF resource == 'customer' AND operation == 'create' THEN SHOW parameter 'email'",
      "IF resource == 'customer' AND operation == 'create' THEN SHOW parameter 'firstname'",
      "IF resource == 'customer' AND operation == 'create' THEN SHOW parameter 'lastname'",
      "IF resource == 'customer' AND operation == 'create' THEN SHOW parameter 'additionalFields'",
      "IF /operation == 'create' THEN SHOW parameter 'website_id'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'customerId'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'email'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'firstName'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'lastName'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'website_id'",
      "IF resource == 'customer' AND operation == 'update' THEN SHOW parameter 'updateFields'",
      "IF resource == 'customer' AND operation IN [\"delete\", \"get\"] THEN SHOW parameter 'customerId'",
      "IF resource == 'customer' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'customer' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'customer' AND operation == 'getAll' THEN SHOW parameter 'filterType'",
      "IF resource == 'customer' AND operation == 'getAll' AND filterType == 'manual' THEN SHOW parameter 'matchType'",
      "IF resource == 'customer' AND operation == 'getAll' AND filterType == 'manual' THEN SHOW parameter 'filters'",
      "IF condition_type IN [\"null\", \"notnull\"] THEN HIDE parameter 'value'",
      "IF resource == 'customer' AND operation == 'getAll' AND filterType == 'json' THEN SHOW parameter 'jsonNotice'",
      "IF resource == 'customer' AND operation == 'getAll' AND filterType == 'json' THEN SHOW parameter 'filterJson'",
      "IF resource == 'customer' AND operation == 'getAll' THEN SHOW parameter 'options'",
      "IF resource == 'invoice' THEN SHOW parameter 'operation'",
