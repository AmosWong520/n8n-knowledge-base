  "configuration_logic": {
    "required_sequence": [
      "authentication",
      "resource",
      "operation",
      "publication",
      "publicationId",
      "title",
      "contentFormat",
      "content",
      "additionalFields",
      "operation",
      "returnAll",
      "limit"
    ],
    "conditional_rules": [
      "IF resource == 'post' THEN SHOW parameter 'operation'",
      "IF resource == 'post' AND operation == 'create' THEN SHOW parameter 'publication'",
      "IF resource == 'post' AND operation == 'create' AND publication == True THEN SHOW parameter 'publicationId'",
      "IF operation == 'create' AND resource == 'post' THEN SHOW parameter 'title'",
      "IF operation == 'create' AND resource == 'post' THEN SHOW parameter 'contentFormat'",
      "IF operation == 'create' AND resource == 'post' THEN SHOW parameter 'content'",
      "IF operation == 'create' AND resource == 'post' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'publication' THEN SHOW parameter 'operation'",
      "IF operation == 'getAll' AND resource == 'publication' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'publication' AND returnAll == False THEN SHOW parameter 'limit'"
    ]
  }
