    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF resource == 'company' THEN SHOW parameter 'operation'",
      "IF resource == 'company' AND operation IN [\"getScorecard\", \"getFactor\", \"getFactorHistorical\", \"getHistoricalScore\", \"getScorePlan\"] THEN SHOW parameter 'scorecardIdentifier'",
      "IF resource == 'company' AND operation == 'getScorePlan' THEN SHOW parameter 'score'",
      "IF resource == 'company' AND operation IN [\"getFactor\", \"getFactorHistorical\", \"getHistoricalScore\", \"getScorePlan\"] THEN SHOW parameter 'returnAll'",
      "IF resource == 'company' AND operation IN [\"getFactor\", \"getFactorHistorical\", \"getHistoricalScore\", \"getScorePlan\"] AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'company' AND operation IN [\"getFactorHistorical\", \"getHistoricalScore\"] THEN SHOW parameter 'simple'",
      "IF resource == 'company' AND operation == 'getFactor' THEN SHOW parameter 'filters'",
      "IF resource == 'company' AND operation IN [\"getFactorHistorical\", \"getHistoricalScore\"] THEN SHOW parameter 'options'",
      "IF resource == 'industry' THEN SHOW parameter 'operation'",
      "IF resource == 'industry' AND operation IN [\"getScore\", \"getFactor\", \"getFactorHistorical\"] THEN SHOW parameter 'industry'",
      "IF resource == 'industry' AND operation IN [\"getFactor\", \"getFactorHistorical\"] THEN SHOW parameter 'returnAll'",
      "IF resource == 'industry' AND operation IN [\"getFactor\", \"getFactorHistorical\"] AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'industry' AND operation IN [\"getFactor\", \"getFactorHistorical\"] THEN SHOW parameter 'simple'",
      "IF resource == 'industry' AND operation == 'getFactorHistorical' THEN SHOW parameter 'options'",
      "IF resource == 'invite' THEN SHOW parameter 'operation'",
      "IF resource == 'invite' AND operation == 'create' THEN SHOW parameter 'email'",
      "IF resource == 'invite' AND operation == 'create' THEN SHOW parameter 'firstName'",
      "IF resource == 'invite' AND operation == 'create' THEN SHOW parameter 'lastName'",
      "IF resource == 'invite' AND operation == 'create' THEN SHOW parameter 'message'",
      "IF resource == 'invite' AND operation == 'create' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'portfolio' THEN SHOW parameter 'operation'",
      "IF resource == 'portfolio' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'portfolio' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'portfolio' AND operation IN [\"update\", \"delete\"] THEN SHOW parameter 'portfolioId'",
      "IF resource == 'portfolio' AND operation IN [\"create\", \"update\"] THEN SHOW parameter 'name'",
      "IF resource == 'portfolio' AND operation IN [\"create\", \"update\"] THEN SHOW parameter 'description'",
      "IF resource == 'portfolio' AND operation IN [\"create\", \"update\"] THEN SHOW parameter 'privacy'",
      "IF resource == 'portfolioCompany' THEN SHOW parameter 'operation'",
      "IF resource == 'portfolioCompany' AND operation IN [\"getAll\", \"add\", \"remove\"] THEN SHOW parameter 'portfolioId'",
      "IF resource == 'portfolioCompany' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'portfolioCompany' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'portfolioCompany' AND operation == 'getAll' THEN SHOW parameter 'filters'",
      "IF resource == 'portfolioCompany' AND operation IN [\"add\", \"remove\"] THEN SHOW parameter 'domain'",
      "IF resource == 'report' THEN SHOW parameter 'operation'",
      "IF resource == 'report' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'report' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'report' AND operation == 'generate' THEN SHOW parameter 'report'",
      "IF resource == 'report' AND operation == 'generate' AND report IN [\"detailed\", \"events-json\", \"full-scorecard-json\", \"issues\", \"partnership\", \"scorecard-footprint\", \"summary\"] THEN SHOW parameter 'scorecardIdentifier'",
      "IF resource == 'report' AND operation == 'generate' AND report == 'portfolio' THEN SHOW parameter 'portfolioId'",
      "IF resource == 'report' AND operation == 'generate' AND report IN [\"detailed\", \"summary\"] THEN SHOW parameter 'branding'",
      "IF resource == 'report' AND operation == 'generate' AND report == 'events-json' THEN SHOW parameter 'date'",
      "IF resource == 'report' AND operation == 'generate' AND report IN [\"issues\", \"portfolio\"] THEN SHOW parameter 'options'",
      "IF resource == 'report' AND operation == 'generate' AND report == 'scorecard-footprint' THEN SHOW parameter 'options'",
