    ],
    "conditional_rules": [
      "IF resource == 'comment' THEN SHOW parameter 'operation'",
      "IF resource == 'comment' AND operation == 'addComment' THEN SHOW parameter 'issueId'",
      "IF resource == 'comment' AND operation == 'addComment' THEN SHOW parameter 'comment'",
      "IF resource == 'comment' AND operation == 'addComment' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'issue' THEN SHOW parameter 'operation'",
      "IF resource == 'issue' AND operation == 'create' THEN SHOW parameter 'teamId'",
      "IF resource == 'issue' AND operation == 'create' THEN SHOW parameter 'title'",
      "IF resource == 'issue' AND operation == 'create' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'issue' AND operation IN [\"addLink\", \"get\", \"delete\"] THEN SHOW parameter 'issueId'",
      "IF resource == 'issue' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'issue' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'issue' AND operation == 'update' THEN SHOW parameter 'issueId'",
      "IF resource == 'issue' AND operation == 'update' AND @version == 1 THEN SHOW parameter 'updateFields'",
      "IF resource == 'issue' AND operation == 'update' THEN SHOW parameter 'updateFields'",
      "IF @version == 1 THEN HIDE parameter 'updateFields'",
      "IF resource == 'issue' AND operation == 'addLink' THEN SHOW parameter 'link'"
