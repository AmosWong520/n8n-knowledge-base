{
  "node_id": "n8n-nodes-base.awsTextract",
  "name": "n8n-nodes-base.awsTextract",
  "version": 1,
  "semantic_context": "Sends data to Amazon Textract",
  "parameters": {
    "authentication": {
      "displayName": "Authentication",
      "name": "authentication",
      "type": "options",
      "options": [
        {
          "name": "AWS (IAM)",
          "value": "iam"
        },
        {
          "name": "AWS (Assume Role)",
          "value": "assumeRole"
        }
      ],
      "default": "iam",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。 与鉴权相关，注意不要在公共日志中输出敏感信息。"
    },
    "operation": {
      "displayName": "Operation",
      "name": "operation",
      "type": "options",
      "noDataExpression": true,
      "options": [
        {
          "name": "Analyze Receipt or Invoice",
          "value": "analyzeExpense"
        },
        {
          "name": "Custom API Call",
          "value": "__CUSTOM_API_CALL__"
        }
      ],
      "default": "analyzeExpense",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "binaryPropertyName": {
      "displayName": "Input Data Field Name",
      "name": "binaryPropertyName",
      "type": "string",
      "default": "data",
      "displayOptions": {
        "show": {
          "operation": [
            "analyzeExpense"
          ]
        }
      },
      "required": true,
      "description": "The name of the input field containing the binary file data to be uploaded. Supported file types: PNG, JPEG.",
      "ai_hint": "The name of the input field containing the binary file data to be uploaded. Supported file types: PNG, JPEG."
    },
    "simple": {
      "displayName": "Simplify",
      "name": "simple",
      "type": "boolean",
      "displayOptions": {
        "show": {
          "operation": [
            "analyzeExpense"
          ]
        }
      },
      "default": true,
      "description": "Whether to return a simplified version of the response instead of the raw data",
      "ai_hint": "布尔值：true 或 false。"
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "authentication",
      "operation",
      "binaryPropertyName",
      "simple"
    ],
    "conditional_rules": [
      "IF operation == 'analyzeExpense' THEN SHOW parameter 'binaryPropertyName'",
      "IF operation == 'analyzeExpense' THEN SHOW parameter 'simple'"
    ]
  }
}