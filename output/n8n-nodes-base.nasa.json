    "conditional_rules": [
      "IF resource == 'astronomyPictureOfTheDay' THEN SHOW parameter 'operation'",
      "IF resource == 'asteroidNeoFeed' THEN SHOW parameter 'operation'",
      "IF resource == 'asteroidNeoLookup' THEN SHOW parameter 'operation'",
