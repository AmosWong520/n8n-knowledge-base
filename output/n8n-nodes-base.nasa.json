      "IF resource == 'astronomyPictureOfTheDay' THEN SHOW parameter 'download'",
      "IF operation == 'get' AND resource == 'astronomyPictureOfTheDay' AND download == True THEN SHOW parameter 'binaryPropertyName'",
      "IF resource == 'astronomyPictureOfTheDay' AND operation == 'get' THEN SHOW parameter 'additionalFields'",
      "IF resource IN [\"asteroidNeoFeed\", \"donkiCoronalMassEjection\", \"donkiGeomagneticStorm\", \"donkiSolarFlare\", \"donkiSolarEnergeticParticle\", \"donkiMagnetopauseCrossing\", \"donkiRadiationBeltEnhancement\", \"donkiHighSpeedStream\", \"donkiWsaEnlilSimulation\", \"donkiNotifications\"] AND operation == 'get' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'donkiInterplanetaryShock' AND operation == 'get' THEN SHOW parameter 'additionalFields'",
      "IF resource IN [\"earthImagery\", \"earthAssets\"] AND operation == 'get' THEN SHOW parameter 'lat'",
      "IF resource IN [\"earthImagery\", \"earthAssets\"] AND operation == 'get' THEN SHOW parameter 'lon'",
      "IF operation == 'get' AND resource == 'earthImagery' THEN SHOW parameter 'binaryPropertyName'",
      "IF resource IN [\"earthImagery\", \"earthAssets\"] AND operation == 'get' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'"
