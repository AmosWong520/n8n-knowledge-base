      "IF resource == 'imageAndVideoLibrary' THEN SHOW parameter 'operation'",
      "IF resource == 'techTransfer' THEN SHOW parameter 'operation'",
      "IF resource == 'twoLineElementSet' THEN SHOW parameter 'operation'",
