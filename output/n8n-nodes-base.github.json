    "conditional_rules": [
      "IF resource == 'organization' THEN SHOW parameter 'operation'",
      "IF resource == 'issue' THEN SHOW parameter 'operation'",
      "IF resource == 'file' THEN SHOW parameter 'operation'",
      "IF resource == 'repository' THEN SHOW parameter 'operation'",
      "IF resource == 'user' THEN SHOW parameter 'operation'",
      "IF resource == 'release' THEN SHOW parameter 'operation'",
      "IF resource == 'review' THEN SHOW parameter 'operation'",
      "IF resource == 'workflow' THEN SHOW parameter 'operation'",
      "IF resource == 'workflow' AND operation == 'dispatchAndWait' THEN SHOW parameter 'webhookNotice'",
      "IF operation IN [\"invite\", \"getUserIssues\"] THEN HIDE parameter 'owner'",
      "IF resource IN [\"user\", \"organization\"] AND operation == 'getRepositories' THEN HIDE parameter 'repository'",
      "IF resource == 'workflow' AND operation IN [\"disable\", \"dispatch\", \"dispatchAndWait\", \"get\", \"getUsage\", \"enable\"] THEN SHOW parameter 'workflowId'",
      "IF resource == 'workflow' AND operation IN [\"dispatch\", \"dispatchAndWait\"] AND @version == {'_cnd': {'lte': 1}} THEN SHOW parameter 'ref'",
      "IF resource == 'workflow' AND operation IN [\"dispatch\", \"dispatchAndWait\"] AND @version == {'_cnd': {'gte': 1.1}} THEN SHOW parameter 'ref'",
      "IF resource == 'workflow' AND operation IN [\"dispatch\", \"dispatchAndWait\"] THEN SHOW parameter 'inputs'",
      "IF resource == 'file' THEN SHOW parameter 'filePath'",
      "IF operation == 'list' THEN HIDE parameter 'filePath'",
      "IF resource == 'file' AND operation == 'list' THEN SHOW parameter 'filePath'",
      "IF operation IN [\"create\", \"edit\"] AND resource == 'file' THEN SHOW parameter 'binaryData'",
      "IF binaryData == False AND operation IN [\"create\", \"edit\"] AND resource == 'file' THEN SHOW parameter 'fileContent'",
      "IF binaryData == True AND operation IN [\"create\", \"edit\"] AND resource == 'file' THEN SHOW parameter 'binaryPropertyName'",
      "IF operation IN [\"create\", \"delete\", \"edit\"] AND resource == 'file' THEN SHOW parameter 'commitMessage'",
      "IF operation IN [\"create\", \"delete\", \"edit\"] AND resource == 'file' THEN SHOW parameter 'additionalParameters'",
      "IF operation == 'get' AND resource == 'file' THEN SHOW parameter 'asBinaryProperty'",
      "IF asBinaryProperty == True AND operation == 'get' AND resource == 'file' THEN SHOW parameter 'binaryPropertyName'",
      "IF operation == 'get' AND resource == 'file' THEN SHOW parameter 'additionalParameters'",
      "IF operation == 'create' AND resource == 'issue' THEN SHOW parameter 'title'",
      "IF operation == 'create' AND resource == 'issue' THEN SHOW parameter 'body'",
      "IF operation == 'create' AND resource == 'issue' THEN SHOW parameter 'labels'",
      "IF operation == 'create' AND resource == 'issue' THEN SHOW parameter 'assignees'",
      "IF operation == 'createComment' AND resource == 'issue' THEN SHOW parameter 'issueNumber'",
      "IF operation == 'createComment' AND resource == 'issue' THEN SHOW parameter 'body'",
      "IF operation == 'edit' AND resource == 'issue' THEN SHOW parameter 'issueNumber'",
      "IF operation == 'edit' AND resource == 'issue' THEN SHOW parameter 'editFields'",
      "IF operation == 'get' AND resource == 'issue' THEN SHOW parameter 'issueNumber'",
      "IF operation == 'lock' AND resource == 'issue' THEN SHOW parameter 'issueNumber'",
      "IF operation == 'lock' AND resource == 'issue' THEN SHOW parameter 'lockReason'",
      "IF operation == 'create' AND resource == 'release' THEN SHOW parameter 'releaseTag'",
      "IF operation == 'create' AND resource == 'release' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'release' AND operation IN [\"get\", \"delete\", \"update\"] THEN SHOW parameter 'release_id'",
      "IF operation == 'update' AND resource == 'release' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'release' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'release' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'repository' AND operation == 'getIssues' THEN SHOW parameter 'returnAll'",
      "IF resource == 'repository' AND operation == 'getIssues' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getIssues' AND resource == 'repository' THEN SHOW parameter 'getRepositoryIssuesFilters'",
      "IF resource == 'repository' AND operation == 'getPullRequests' THEN SHOW parameter 'returnAll'",
      "IF resource == 'repository' AND operation == 'getPullRequests' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getPullRequests' AND resource == 'repository' THEN SHOW parameter 'getRepositoryPullRequestsFilters'",
      "IF operation IN [\"get\", \"update\"] AND resource == 'review' THEN SHOW parameter 'pullRequestNumber'",
      "IF operation IN [\"get\", \"update\"] AND resource == 'review' THEN SHOW parameter 'reviewId'",
      "IF operation == 'getAll' AND resource == 'review' THEN SHOW parameter 'pullRequestNumber'",
      "IF resource == 'review' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'review' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'create' AND resource == 'review' THEN SHOW parameter 'pullRequestNumber'",
      "IF operation == 'create' AND resource == 'review' THEN SHOW parameter 'event'",
      "IF operation == 'create' AND resource == 'review' AND event IN [\"requestChanges\", \"comment\"] THEN SHOW parameter 'body'",
      "IF operation == 'create' AND resource == 'review' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'update' AND resource == 'review' THEN SHOW parameter 'body'",
      "IF resource == 'user' AND operation == 'getRepositories' THEN SHOW parameter 'returnAll'",
      "IF resource == 'user' AND operation == 'getRepositories' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'invite' AND resource == 'user' THEN SHOW parameter 'organization'",
      "IF operation == 'invite' AND resource == 'user' THEN SHOW parameter 'email'",
      "IF resource == 'organization' AND operation == 'getRepositories' THEN SHOW parameter 'returnAll'",
      "IF resource == 'organization' AND operation == 'getRepositories' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getUserIssues' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getUserIssues' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getUserIssues' THEN SHOW parameter 'getUserIssuesFilters'"
