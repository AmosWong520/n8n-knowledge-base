  "configuration_logic": {
    "required_sequence": [
      "authentication",
      "resource",
      "operation",
      "certificateArn",
      "bucketName",
      "certificateKey",
      "returnAll",
      "limit",
      "options"
    ],
    "conditional_rules": [
      "IF resource == 'certificate' THEN SHOW parameter 'operation'",
      "IF resource == 'certificate' AND operation IN [\"renew\", \"get\", \"delete\", \"getMetadata\"] THEN SHOW parameter 'certificateArn'",
      "IF resource == 'certificate' AND operation == 'delete' THEN SHOW parameter 'bucketName'",
      "IF resource == 'certificate' AND operation == 'delete' THEN SHOW parameter 'certificateKey'",
      "IF operation == 'getMany' AND resource == 'certificate' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getMany' AND resource == 'certificate' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'certificate' AND operation == 'getMany' THEN SHOW parameter 'options'"
    ]
  }
