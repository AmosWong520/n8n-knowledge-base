    "required_sequence": [
      "resource",
      "operation",
      "userId",
      "firstName",
      "lastName",
      "login",
      "email",
      "activate",
      "getCreateFields",
      "getUpdateFields",
      "searchQuery",
      "limit",
      "returnAll",
      "simplify",
      "sendEmail",
      "requestOptions"
    ],
    "conditional_rules": [
      "IF resource == 'user' THEN SHOW parameter 'operation'",
      "IF resource == 'user' AND operation IN [\"get\", \"update\", \"delete\"] THEN SHOW parameter 'userId'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'firstName'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'lastName'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'login'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'email'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'activate'",
      "IF resource == 'user' AND operation == 'create' THEN SHOW parameter 'getCreateFields'",
      "IF resource == 'user' AND operation == 'update' THEN SHOW parameter 'getUpdateFields'",
      "IF resource == 'user' AND operation == 'getAll' THEN SHOW parameter 'searchQuery'",
      "IF resource == 'user' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'user' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'user' AND operation IN [\"get\", \"getAll\"] THEN SHOW parameter 'simplify'",
      "IF resource == 'user' AND operation == 'delete' THEN SHOW parameter 'sendEmail'"
    ]
  }
