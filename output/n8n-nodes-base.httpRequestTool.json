  "configuration_logic": {
    "required_sequence": [
      "preBuiltAgentsCalloutHttpRequest",
      "toolDescription",
      "curlImport",
      "method",
      "url",
      "authentication",
      "nodeCredentialType",
      "googleApiWarning",
      "genericAuthType",
      "provideSslCertificates",
      "provideSslCertificatesNotice",
      "sslCertificate",
      "sendQuery",
      "specifyQuery",
      "queryParameters",
      "jsonQuery",
      "sendHeaders",
      "specifyHeaders",
      "headerParameters",
      "jsonHeaders",
      "sendBody",
      "contentType",
      "specifyBody",
      "bodyParameters",
      "jsonBody",
      "bodyParameters",
      "specifyBody",
      "bodyParameters",
      "body",
      "inputDataFieldName",
      "rawContentType",
      "body",
      "options",
      "optimizeResponse",
      "responseType",
      "dataField",
      "fieldsToInclude",
      "fields",
      "cssSelector",
      "onlyContent",
      "elementsToOmit",
      "truncateResponse",
      "maxLength",
      "infoMessage"
    ],
    "conditional_rules": [
      "IF authentication == 'predefinedCredentialType' THEN SHOW parameter 'nodeCredentialType'",
      "IF nodeCredentialType == 'googleApi' THEN SHOW parameter 'googleApiWarning'",
      "IF authentication == 'genericCredentialType' THEN SHOW parameter 'genericAuthType'",
      "IF provideSslCertificates == True THEN SHOW parameter 'provideSslCertificatesNotice'",
      "IF provideSslCertificates == True THEN SHOW parameter 'sslCertificate'",
      "IF sendQuery == True THEN SHOW parameter 'specifyQuery'",
      "IF sendQuery == True AND specifyQuery == 'keypair' THEN SHOW parameter 'queryParameters'",
      "IF sendQuery == True AND specifyQuery == 'json' THEN SHOW parameter 'jsonQuery'",
      "IF sendHeaders == True THEN SHOW parameter 'specifyHeaders'",
      "IF sendHeaders == True AND specifyHeaders == 'keypair' THEN SHOW parameter 'headerParameters'",
      "IF sendHeaders == True AND specifyHeaders == 'json' THEN SHOW parameter 'jsonHeaders'",
      "IF sendBody == True THEN SHOW parameter 'contentType'",
      "IF sendBody == True AND contentType == 'json' THEN SHOW parameter 'specifyBody'",
      "IF sendBody == True AND contentType == 'json' AND specifyBody == 'keypair' THEN SHOW parameter 'bodyParameters'",
      "IF sendBody == True AND contentType == 'json' AND specifyBody == 'json' THEN SHOW parameter 'jsonBody'",
      "IF sendBody == True AND contentType == 'multipart-form-data' THEN SHOW parameter 'bodyParameters'",
      "IF parameterType == 'formData' THEN SHOW parameter 'value'",
      "IF parameterType == 'formBinaryData' THEN SHOW parameter 'inputDataFieldName'",
      "IF sendBody == True AND contentType == 'form-urlencoded' THEN SHOW parameter 'specifyBody'",
      "IF sendBody == True AND contentType == 'form-urlencoded' AND specifyBody == 'keypair' THEN SHOW parameter 'bodyParameters'",
      "IF sendBody == True AND specifyBody == 'string' THEN SHOW parameter 'body'",
      "IF sendBody == True AND contentType == 'binaryData' THEN SHOW parameter 'inputDataFieldName'",
      "IF sendBody == True AND contentType == 'raw' THEN SHOW parameter 'rawContentType'",
      "IF sendBody == True AND contentType == 'raw' THEN SHOW parameter 'body'",
      "IF /sendQuery == True THEN SHOW parameter 'queryParameterArrays'",
      "IF @version IN [1, 2, 3] THEN SHOW parameter 'redirect'",
      "IF followRedirects == True THEN SHOW parameter 'maxRedirects'",
      "IF @version IN [1, 2, 3] THEN HIDE parameter 'redirect'",
      "IF responseFormat IN [\"file\", \"text\"] THEN SHOW parameter 'outputPropertyName'",
      "IF paginationMode == 'off' THEN HIDE parameter 'webhookNotice'",
      "IF paginationMode == 'responseContainsNextURL' THEN SHOW parameter 'nextURL'",
      "IF paginationMode == 'updateAParameterInEachRequest' THEN SHOW parameter 'parameters'",
      "IF paginationMode == 'off' THEN HIDE parameter 'paginationCompleteWhen'",
      "IF paginationCompleteWhen == 'receiveSpecificStatusCodes' THEN SHOW parameter 'statusCodesWhenComplete'",
      "IF paginationCompleteWhen == 'other' THEN SHOW parameter 'completeExpression'",
      "IF paginationMode == 'off' THEN HIDE parameter 'limitPagesFetched'",
      "IF limitPagesFetched == True THEN SHOW parameter 'maxRequests'",
      "IF paginationMode == 'off' THEN HIDE parameter 'requestInterval'",
      "IF @tool == True THEN SHOW parameter 'optimizeResponse'",
      "IF optimizeResponse == True AND @tool == True THEN SHOW parameter 'responseType'",
      "IF optimizeResponse == True AND responseType == 'json' AND @tool == True THEN SHOW parameter 'dataField'",
      "IF optimizeResponse == True AND responseType == 'json' AND @tool == True THEN SHOW parameter 'fieldsToInclude'",
      "IF optimizeResponse == True AND responseType == 'json' AND @tool == True THEN SHOW parameter 'fields'",
      "IF fieldsToInclude == 'all' THEN HIDE parameter 'fields'",
      "IF optimizeResponse == True AND responseType == 'html' AND @tool == True THEN SHOW parameter 'cssSelector'",
      "IF optimizeResponse == True AND responseType == 'html' AND @tool == True THEN SHOW parameter 'onlyContent'",
      "IF optimizeResponse == True AND responseType == 'html' AND onlyContent == True AND @tool == True THEN SHOW parameter 'elementsToOmit'",
      "IF optimizeResponse == True AND responseType IN [\"text\", \"html\"] AND @tool == True THEN SHOW parameter 'truncateResponse'",
      "IF optimizeResponse == True AND responseType IN [\"text\", \"html\"] AND truncateResponse == True AND @tool == True THEN SHOW parameter 'maxLength'"
    ]
  }
}