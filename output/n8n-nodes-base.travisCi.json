      "IF resource == 'build' AND operation == 'get' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'getAll' AND resource == 'build' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'build' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'build' AND operation == 'getAll' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'restart' AND resource == 'build' THEN SHOW parameter 'buildId'",
      "IF operation == 'trigger' AND resource == 'build' THEN SHOW parameter 'slug'",
      "IF operation == 'trigger' AND resource == 'build' THEN SHOW parameter 'branch'",
