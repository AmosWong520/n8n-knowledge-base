  "configuration_logic": {
    "required_sequence": [
      "toolDescription",
      "action",
      "type",
      "binaryData",
      "binaryPropertyName",
      "value",
      "dataPropertyName",
      "encoding",
      "type",
      "value",
      "dataPropertyName",
      "secret",
      "encoding",
      "value",
      "dataPropertyName",
      "algorithm",
      "encoding",
      "privateKey",
      "dataPropertyName",
      "encodingType",
      "stringLength"
    ],
    "conditional_rules": [
      "IF action == 'hash' THEN SHOW parameter 'type'",
      "IF action IN [\"hash\", \"hmac\"] THEN SHOW parameter 'binaryData'",
      "IF action IN [\"hash\", \"hmac\"] AND binaryData == True THEN SHOW parameter 'binaryPropertyName'",
      "IF action == 'hash' AND binaryData == False THEN SHOW parameter 'value'",
      "IF action == 'hash' THEN SHOW parameter 'dataPropertyName'",
      "IF action == 'hash' THEN SHOW parameter 'encoding'",
      "IF action == 'hmac' THEN SHOW parameter 'type'",
      "IF action == 'hmac' AND binaryData == False THEN SHOW parameter 'value'",
      "IF action == 'hmac' THEN SHOW parameter 'dataPropertyName'",
      "IF action == 'hmac' THEN SHOW parameter 'secret'",
      "IF action == 'hmac' THEN SHOW parameter 'encoding'",
      "IF action == 'sign' THEN SHOW parameter 'value'",
      "IF action == 'sign' THEN SHOW parameter 'dataPropertyName'",
      "IF action == 'sign' THEN SHOW parameter 'algorithm'",
      "IF action == 'sign' THEN SHOW parameter 'encoding'",
      "IF action == 'sign' THEN SHOW parameter 'privateKey'",
      "IF action == 'generate' THEN SHOW parameter 'dataPropertyName'",
      "IF action == 'generate' THEN SHOW parameter 'encodingType'",
      "IF action == 'generate' AND encodingType IN [\"ascii\", \"base64\", \"hex\"] THEN SHOW parameter 'stringLength'"
    ]
