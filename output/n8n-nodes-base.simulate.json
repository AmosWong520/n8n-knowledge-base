{
  "node_id": "n8n-nodes-base.simulate",
  "name": "n8n-nodes-base.simulate",
  "version": 1,
  "semantic_context": "Simulate a node",
  "parameters": {
    "icon": {
      "displayName": "Icon to Display on Canvas",
      "name": "icon",
      "type": "options",
      "description": "Select a type of node to show corresponding icon",
      "default": "n8n-nodes-base.noOp",
      "typeOptions": {
        "loadOptionsMethod": "getNodeTypes"
      },
      "ai_hint": "Select a type of node to show corresponding icon"
    },
    "subtitle": {
      "displayName": "Subtitle",
      "name": "subtitle",
      "type": "string",
      "default": "",
      "placeholder": "e.g. 'record: read'"
    },
    "output": {
      "displayName": "Output",
      "name": "output",
      "type": "options",
      "default": "all",
      "noDataExpression": true,
      "options": [
        {
          "name": "Returns all input items",
          "value": "all"
        },
        {
          "name": "Specify how many of input items to return",
          "value": "specify"
        },
        {
          "name": "Specify output as JSON",
          "value": "custom"
        }
      ],
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "numberOfItems": {
      "displayName": "Number of Items",
      "name": "numberOfItems",
      "type": "number",
      "default": 1,
      "description": "Number input of items to return, if greater then input length all items will be returned",
      "displayOptions": {
        "show": {
          "output": [
            "specify"
          ]
        }
      },
      "typeOptions": {
        "minValue": 1
      },
      "ai_hint": "Number input of items to return, if greater then input length all items will be returned"
    },
    "jsonOutput": {
      "displayName": "JSON",
      "name": "jsonOutput",
      "type": "json",
      "typeOptions": {
        "rows": 5
      },
      "default": "[\n  {\n  \"my_field_1\": \"value\",\n  \"my_field_2\": 1\n  }\n]",
      "validateType": "array",
      "displayOptions": {
        "show": {
          "output": [
            "custom"
          ]
        }
      }
    },
    "executionDuration": {
      "displayName": "Execution Duration (MS)",
      "name": "executionDuration",
      "type": "number",
      "default": 150,
      "description": "Execution duration in milliseconds",
      "typeOptions": {
        "minValue": 0
      },
      "ai_hint": "Execution duration in milliseconds"
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "icon",
      "subtitle",
      "output",
      "numberOfItems",
      "jsonOutput",
      "executionDuration"
    ],
    "conditional_rules": [
      "IF output == 'specify' THEN SHOW parameter 'numberOfItems'",
      "IF output == 'custom' THEN SHOW parameter 'jsonOutput'"
    ]
  }
}