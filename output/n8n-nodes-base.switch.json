{
  "node_id": "n8n-nodes-base.switch",
  "name": "n8n-nodes-base.switch",
  "version": [
    1
  ],
  "semantic_context": "Route items depending on defined expression or rules",
  "parameters": {
    "mode": {
      "displayName": "Mode",
      "name": "mode",
      "type": "options",
      "options": [
        {
          "name": "Expression",
          "value": "expression",
          "description": "Expression decides how to route data"
        },
        {
          "name": "Rules",
          "value": "rules",
          "description": "Rules decide how to route data"
        }
      ],
      "default": "rules",
      "description": "How data should be routed",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "output": {
      "displayName": "Output",
      "name": "output",
      "type": "number",
      "typeOptions": {
        "minValue": 0,
        "maxValue": 3
      },
      "displayOptions": {
        "show": {
          "mode": [
            "expression"
          ]
        }
      },
      "default": 0,
      "description": "The index of output to which to send data to",
      "ai_hint": "The index of output to which to send data to"
    },
    "dataType": {
      "displayName": "Data Type",
      "name": "dataType",
      "type": "options",
      "displayOptions": {
        "show": {
          "mode": [
            "rules"
          ]
        }
      },
      "options": [
        {
          "name": "Boolean",
          "value": "boolean"
        },
        {
          "name": "Date & Time",
          "value": "dateTime"
        },
        {
          "name": "Number",
          "value": "number"
        },
        {
          "name": "String",
          "value": "string"
        }
      ],
      "default": "number",
      "description": "The type of data to route on",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "value1": {
      "displayName": "Value 1",
      "name": "value1",
      "type": "string",
      "displayOptions": {
        "show": {
          "dataType": [
            "string"
          ],
          "mode": [
            "rules"
          ]
        }
      },
      "default": "",
      "description": "The value to compare with the second one",
      "ai_hint": "The value to compare with the second one"
    },
    "rules": {
      "displayName": "Routing Rules",
      "name": "rules",
      "placeholder": "Add Routing Rule",
      "type": "fixedCollection",
      "typeOptions": {
        "multipleValues": true
      },
      "displayOptions": {
        "show": {
          "dataType": [
            "string"
          ],
          "mode": [
            "rules"
          ]
        }
      },
      "default": {},
      "options": [
        {
          "name": "rules",
          "displayName": "Strings",
          "values": [
            {
              "displayName": "Operation",
              "name": "operation",
              "type": "options",
              "options": [
                {
                  "name": "Contains",
                  "value": "contains"
                },
                {
                  "name": "Not Contains",
                  "value": "notContains"
                },
                {
                  "name": "Ends With",
                  "value": "endsWith"
                },
                {
                  "name": "Not Ends With",
                  "value": "notEndsWith"
                },
                {
                  "name": "Equal",
                  "value": "equal"
                },
                {
                  "name": "Not Equal",
                  "value": "notEqual"
                },
                {
                  "name": "Regex Match",
                  "value": "regex"
                },
                {
                  "name": "Regex Not Match",
                  "value": "notRegex"
                },
                {
                  "name": "Starts With",
                  "value": "startsWith"
                },
                {
                  "name": "Not Starts With",
                  "value": "notStartsWith"
                }
              ],
              "default": "equal",
              "description": "Operation to decide where the data should be mapped to"
            },
            {
              "displayName": "Value 2",
              "name": "value2",
              "type": "string",
              "displayOptions": {
                "hide": {
                  "operation": [
                    "regex",
                    "notRegex"
                  ]
                }
              },
              "default": "",
              "description": "The value to compare with the first one"
            },
            {
              "displayName": "Regex",
              "name": "value2",
              "type": "string",
              "displayOptions": {
                "show": {
                  "operation": [
                    "regex",
                    "notRegex"
                  ]
                }
              },
              "default": "",
              "placeholder": "/text/i",
              "description": "The regex which has to match"
            },
            {
              "displayName": "Output",
              "name": "output",
              "type": "number",
              "typeOptions": {
                "minValue": 0,
                "maxValue": 3
              },
              "default": 0,
              "description": "The index of output to which to send data to if rule matches"
            }
          ]
        }
      ],
      "ai_hint": "此参数为 fixedCollection，包含嵌套字段，不要扁平化。"
    },
    "fallbackOutput": {
      "displayName": "Fallback Output",
      "name": "fallbackOutput",
      "type": "options",
      "displayOptions": {
        "show": {
          "mode": [
            "rules"
          ]
        }
      },
      "options": [
        {
          "name": "None",
          "value": -1
        },
        {
          "name": "0",
          "value": 0
        },
        {
          "name": "1",
          "value": 1
        },
        {
          "name": "2",
          "value": 2
        },
        {
          "name": "3",
          "value": 3
        }
      ],
      "default": -1,
      "description": "The output to which to route all items which do not match any of the rules",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "mode",
      "output",
      "dataType",
      "value1",
      "rules",
      "value1",
      "rules",
      "value1",
      "rules",
      "value1",
      "rules",
      "fallbackOutput"
    ],
    "conditional_rules": [
      "IF mode == 'expression' THEN SHOW parameter 'output'",
      "IF mode == 'rules' THEN SHOW parameter 'dataType'",
      "IF dataType == 'boolean' AND mode == 'rules' THEN SHOW parameter 'value1'",
      "IF dataType == 'boolean' AND mode == 'rules' THEN SHOW parameter 'rules'",
      "IF dataType == 'dateTime' AND mode == 'rules' THEN SHOW parameter 'value1'",
      "IF dataType == 'dateTime' AND mode == 'rules' THEN SHOW parameter 'rules'",
      "IF dataType == 'number' AND mode == 'rules' THEN SHOW parameter 'value1'",
      "IF dataType == 'number' AND mode == 'rules' THEN SHOW parameter 'rules'",
      "IF dataType == 'string' AND mode == 'rules' THEN SHOW parameter 'value1'",
      "IF dataType == 'string' AND mode == 'rules' THEN SHOW parameter 'rules'",
      "IF operation IN [\"regex\", \"notRegex\"] THEN HIDE parameter 'value2'",
      "IF operation IN [\"regex\", \"notRegex\"] THEN SHOW parameter 'value2'",
      "IF mode == 'rules' THEN SHOW parameter 'fallbackOutput'"
    ]
  }
}