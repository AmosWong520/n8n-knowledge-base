  "configuration_logic": {
    "required_sequence": [
      "resource",
      "operation",
      "type",
      "title",
      "body",
      "url",
      "binaryPropertyName",
      "target",
      "value",
      "value",
      "pushId",
      "returnAll",
      "limit",
      "filters",
      "pushId",
      "dismissed"
    ],
    "conditional_rules": [
      "IF resource == 'push' THEN SHOW parameter 'operation'",
      "IF resource == 'push' AND operation == 'create' THEN SHOW parameter 'type'",
      "IF resource == 'push' AND operation == 'create' AND type IN [\"note\", \"link\"] THEN SHOW parameter 'title'",
      "IF resource == 'push' AND operation == 'create' AND type IN [\"note\", \"link\", \"file\"] THEN SHOW parameter 'body'",
      "IF resource == 'push' AND operation == 'create' AND type == 'link' THEN SHOW parameter 'url'",
      "IF resource == 'push' AND operation == 'create' AND type == 'file' THEN SHOW parameter 'binaryPropertyName'",
      "IF resource == 'push' AND operation == 'create' THEN SHOW parameter 'target'",
      "IF resource == 'push' AND operation == 'create' THEN SHOW parameter 'value'",
      "IF target IN [\"default\", \"device_iden\"] THEN HIDE parameter 'value'",
      "IF resource == 'push' AND operation == 'create' AND target == 'device_iden' THEN SHOW parameter 'value'",
      "IF resource == 'push' AND operation == 'delete' THEN SHOW parameter 'pushId'",
      "IF operation == 'getAll' AND resource == 'push' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'push' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'push' AND operation == 'getAll' THEN SHOW parameter 'filters'",
      "IF resource == 'push' AND operation == 'update' THEN SHOW parameter 'pushId'",
      "IF resource == 'push' AND operation == 'update' THEN SHOW parameter 'dismissed'"
    ]
  }
