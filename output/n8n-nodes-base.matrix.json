      "preset",
      "roomAlias",
      "roomIdOrAlias",
      "roomId",
      "roomId",
      "userId",
      "roomId",
      "userId",
      "reason",
      "operation",
      "roomId",
      "filters"
    ],
    "conditional_rules": [
      "IF resource == 'account' THEN SHOW parameter 'operation'",
      "IF resource == 'event' THEN SHOW parameter 'operation'",
      "IF operation == 'get' AND resource == 'event' THEN SHOW parameter 'roomId'",
      "IF operation == 'get' AND resource == 'event' THEN SHOW parameter 'eventId'",
      "IF resource == 'media' THEN SHOW parameter 'operation'",
      "IF resource == 'media' AND operation == 'upload' THEN SHOW parameter 'roomId'",
      "IF resource == 'media' AND operation == 'upload' THEN SHOW parameter 'binaryPropertyName'",
      "IF resource == 'media' AND operation == 'upload' THEN SHOW parameter 'mediaType'",
      "IF resource == 'media' AND operation == 'upload' THEN SHOW parameter 'additionalFields'",
      "IF resource == 'message' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'message' THEN SHOW parameter 'roomId'",
      "IF operation == 'create' AND resource == 'message' THEN SHOW parameter 'text'",
      "IF operation == 'create' AND resource == 'message' THEN SHOW parameter 'messageType'",
      "IF operation == 'create' AND resource == 'message' THEN SHOW parameter 'messageFormat'",
      "IF resource == 'message' AND operation == 'create' AND messageFormat == 'org.matrix.custom.html' THEN SHOW parameter 'fallbackText'",
      "IF resource == 'message' AND operation == 'getAll' THEN SHOW parameter 'roomId'",
      "IF resource == 'message' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'message' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'message' AND operation == 'getAll' THEN SHOW parameter 'otherOptions'",
      "IF resource == 'room' THEN SHOW parameter 'operation'",
      "IF resource == 'room' AND operation == 'create' THEN SHOW parameter 'roomName'",
      "IF resource == 'room' AND operation == 'create' THEN SHOW parameter 'preset'",
      "IF resource == 'room' AND operation == 'create' THEN SHOW parameter 'roomAlias'",
      "IF resource == 'room' AND operation == 'join' THEN SHOW parameter 'roomIdOrAlias'",
      "IF resource == 'room' AND operation == 'leave' THEN SHOW parameter 'roomId'",
      "IF resource == 'room' AND operation == 'invite' THEN SHOW parameter 'roomId'",
      "IF resource == 'room' AND operation == 'invite' THEN SHOW parameter 'userId'",
      "IF resource == 'room' AND operation == 'kick' THEN SHOW parameter 'roomId'",
      "IF resource == 'room' AND operation == 'kick' THEN SHOW parameter 'userId'",
      "IF resource == 'room' AND operation == 'kick' THEN SHOW parameter 'reason'",
      "IF resource == 'roomMember' THEN SHOW parameter 'operation'",
      "IF resource == 'roomMember' AND operation == 'getAll' THEN SHOW parameter 'roomId'",
      "IF resource == 'roomMember' AND operation == 'getAll' THEN SHOW parameter 'filters'"
    ]
  }
