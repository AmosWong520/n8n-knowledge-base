  "configuration_logic": {
    "required_sequence": [
      "resource",
      "operation",
      "operation",
      "roomId",
      "eventId",
      "operation",
      "roomId",
      "binaryPropertyName",
      "mediaType",
      "additionalFields",
      "operation",
      "roomId",
      "text",
      "messageType",
      "messageFormat",
