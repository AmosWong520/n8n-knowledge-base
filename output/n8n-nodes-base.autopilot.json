      "contactId",
      "returnAll",
      "limit",
      "operation",
      "name",
      "returnAll",
      "limit"
    ],
    "conditional_rules": [
      "IF resource == 'contact' THEN SHOW parameter 'operation'",
      "IF operation == 'upsert' AND resource == 'contact' THEN SHOW parameter 'email'",
      "IF operation == 'upsert' AND resource == 'contact' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'delete' AND resource == 'contact' THEN SHOW parameter 'contactId'",
      "IF operation == 'get' AND resource == 'contact' THEN SHOW parameter 'contactId'",
      "IF operation == 'getAll' AND resource == 'contact' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'contact' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'contactJourney' THEN SHOW parameter 'operation'",
      "IF operation == 'add' AND resource == 'contactJourney' THEN SHOW parameter 'triggerId'",
      "IF operation == 'add' AND resource == 'contactJourney' THEN SHOW parameter 'contactId'",
      "IF resource == 'contactList' THEN SHOW parameter 'operation'",
      "IF operation IN [\"add\", \"remove\", \"exist\", \"getAll\"] AND resource == 'contactList' THEN SHOW parameter 'listId'",
      "IF operation IN [\"add\", \"remove\", \"exist\"] AND resource == 'contactList' THEN SHOW parameter 'contactId'",
      "IF operation == 'getAll' AND resource == 'contactList' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'contactList' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF resource == 'list' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'list' THEN SHOW parameter 'name'",
      "IF operation == 'getAll' AND resource == 'list' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'list' AND returnAll == False THEN SHOW parameter 'limit'"
    ]
