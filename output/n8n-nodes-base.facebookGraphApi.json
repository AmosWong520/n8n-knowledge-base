  "configuration_logic": {
    "required_sequence": [
      "hostUrl",
      "httpRequestMethod",
      "graphApiVersion",
      "node",
      "edge",
      "allowUnauthorizedCerts",
      "sendBinaryData",
      "binaryPropertyName",
      "options"
    ],
    "conditional_rules": [
      "IF httpRequestMethod IN [\"POST\", \"PUT\"] THEN SHOW parameter 'sendBinaryData'",
      "IF httpRequestMethod IN [\"POST\", \"PUT\"] THEN SHOW parameter 'binaryPropertyName'",
      "IF sendBinaryData == False THEN HIDE parameter 'binaryPropertyName'",
      "IF /httpRequestMethod == 'GET' THEN SHOW parameter 'fields'"
    ]
  }
