  "configuration_logic": {
    "required_sequence": [
      "authentication",
      "service",
      "options",
      "conditions"
    ],
    "conditional_rules": [
      "IF service == 'support' THEN SHOW parameter 'options'",
      "IF service == 'support' THEN SHOW parameter 'conditions'",
      "IF resource == 'ticket' THEN SHOW parameter 'field'",
      "IF field == 'assignee' THEN HIDE parameter 'operation'",
      "IF field == 'assignee' THEN SHOW parameter 'operation'",
      "IF field == 'status' THEN SHOW parameter 'value'",
      "IF operation IN [\"changed\", \"not_changed\"] AND field IN [\"assignee\", \"group\", \"priority\", \"type\"] THEN HIDE parameter 'value'",
      "IF field == 'type' THEN SHOW parameter 'value'",
      "IF operation IN [\"changed\", \"not_changed\"] AND field IN [\"assignee\", \"group\", \"priority\", \"status\"] THEN HIDE parameter 'value'",
      "IF field == 'priority' THEN SHOW parameter 'value'",
      "IF operation IN [\"changed\", \"not_changed\"] AND field IN [\"assignee\", \"group\", \"type\", \"status\"] THEN HIDE parameter 'value'",
      "IF field == 'group' THEN SHOW parameter 'value'",
      "IF field IN [\"assignee\", \"priority\", \"type\", \"status\"] THEN HIDE parameter 'value'",
      "IF field == 'assignee' THEN SHOW parameter 'value'",
      "IF field IN [\"group\", \"priority\", \"type\", \"status\"] THEN HIDE parameter 'value'"
    ]
