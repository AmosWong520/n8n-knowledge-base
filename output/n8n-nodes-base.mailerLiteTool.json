  "configuration_logic": {
    "required_sequence": [
      "descriptionType",
      "toolDescription",
      "resource",
      "operation",
      "email",
      "subscriberId",
      "additionalFields",
      "subscriberId",
      "subscriberId",
      "returnAll",
      "limit",
      "filters"
    ],
    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF resource == 'subscriber' THEN SHOW parameter 'operation'",
      "IF resource == 'subscriber' AND operation == 'create' THEN SHOW parameter 'email'",
      "IF resource == 'subscriber' AND operation == 'update' THEN SHOW parameter 'subscriberId'",
      "IF resource == 'subscriber' AND operation IN [\"update\", \"create\"] THEN SHOW parameter 'additionalFields'",
      "IF resource == 'subscriber' AND operation == 'delete' THEN SHOW parameter 'subscriberId'",
      "IF resource == 'subscriber' AND operation == 'get' THEN SHOW parameter 'subscriberId'",
      "IF resource == 'subscriber' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'subscriber' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getAll' AND resource == 'subscriber' THEN SHOW parameter 'filters'"
    ]
  }
