      "resource",
      "operation",
      "from",
      "to",
      "language",
      "message",
      "options"
    ],
    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF resource IN [\"sms\", \"voice\"] THEN SHOW parameter 'operation'",
      "IF operation == 'send' AND resource IN [\"sms\", \"voice\"] THEN SHOW parameter 'from'",
      "IF operation == 'send' AND resource IN [\"sms\", \"voice\"] THEN SHOW parameter 'to'",
      "IF operation == 'send' AND resource == 'voice' THEN SHOW parameter 'language'",
      "IF operation == 'send' AND resource IN [\"sms\", \"voice\"] THEN SHOW parameter 'message'",
      "IF operation == 'send' AND resource == 'sms' THEN SHOW parameter 'options'"
