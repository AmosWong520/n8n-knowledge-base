    ],
    "conditional_rules": [
      "IF resume == 'form' THEN SHOW parameter 'incomingAuthentication'",
      "IF resume == 'webhook' THEN SHOW parameter 'incomingAuthentication'",
      "IF resume == 'specificTime' THEN SHOW parameter 'dateTime'",
      "IF resume == 'timeInterval' AND @version == 1 THEN SHOW parameter 'amount'",
      "IF resume == 'timeInterval' THEN SHOW parameter 'amount'",
      "IF @version == 1 THEN HIDE parameter 'amount'",
      "IF resume == 'timeInterval' AND @version == 1 THEN SHOW parameter 'unit'",
      "IF resume == 'timeInterval' THEN SHOW parameter 'unit'",
      "IF @version == 1 THEN HIDE parameter 'unit'",
      "IF resume == 'webhook' THEN SHOW parameter 'webhookNotice'",
      "IF resume == 'form' THEN SHOW parameter 'formNotice'",
      "IF resume == 'form' THEN SHOW parameter 'formTitle'",
      "IF resume == 'form' THEN SHOW parameter 'formDescription'",
      "IF resume == 'form' THEN SHOW parameter 'formFields'",
      "IF fieldType IN [\"hiddenField\", \"html\"] THEN HIDE parameter 'fieldLabel'",
      "IF fieldType == 'hiddenField' THEN SHOW parameter 'fieldName'",
      "IF fieldType == 'html' THEN SHOW parameter 'elementName'",
      "IF fieldType IN [\"dropdown\", \"date\", \"file\", \"html\", \"hiddenField\", \"radio\", \"checkbox\"] THEN HIDE parameter 'placeholder'",
      "IF fieldType IN [\"text\", \"number\", \"email\", \"textarea\"] THEN SHOW parameter 'defaultValue'",
      "IF fieldType == 'date' THEN SHOW parameter 'defaultValue'",
      "IF fieldType IN [\"dropdown\", \"radio\"] THEN SHOW parameter 'defaultValue'",
      "IF fieldType == 'checkbox' THEN SHOW parameter 'defaultValue'",
      "IF fieldType == 'hiddenField' THEN SHOW parameter 'fieldValue'",
      "IF fieldType == 'dropdown' THEN SHOW parameter 'fieldOptions'",
      "IF fieldType == 'checkbox' THEN SHOW parameter 'fieldOptions'",
      "IF fieldType == 'radio' THEN SHOW parameter 'fieldOptions'",
      "IF multiselect == True AND fieldType == 'dropdown' AND @version == {'_cnd': {'lt': 2.3}} THEN SHOW parameter 'multiselectLegacyNotice'",
      "IF fieldType == 'dropdown' AND @version == {'_cnd': {'lt': 2.3}} THEN SHOW parameter 'multiselect'",
      "IF fieldType == 'checkbox' THEN SHOW parameter 'limitSelection'",
      "IF fieldType == 'checkbox' AND limitSelection == 'exact' THEN SHOW parameter 'numberOfSelections'",
      "IF fieldType == 'checkbox' AND limitSelection == 'range' THEN SHOW parameter 'minSelections'",
      "IF fieldType == 'checkbox' AND limitSelection == 'range' THEN SHOW parameter 'maxSelections'",
      "IF fieldType == 'html' THEN SHOW parameter 'html'",
      "IF fieldType == 'file' THEN SHOW parameter 'multipleFiles'",
      "IF fieldType == 'file' THEN SHOW parameter 'acceptFileTypes'",
      "IF fieldType == 'date' THEN SHOW parameter 'formatDate'",
      "IF fieldType IN [\"html\", \"hiddenField\"] THEN HIDE parameter 'requiredField'",
      "IF resume == 'form' THEN SHOW parameter 'responseMode'",
      "IF resume == 'webhook' THEN SHOW parameter 'httpMethod'",
      "IF resume == 'webhook' THEN SHOW parameter 'responseCode'",
      "IF responseMode == 'responseNode' THEN HIDE parameter 'responseCode'",
      "IF @version IN [1, 1.1, 2] AND resume == 'webhook' THEN SHOW parameter 'responseMode'",
      "IF responseMode == 'lastNode' AND resume == 'webhook' THEN SHOW parameter 'responseData'",
      "IF responseData == 'firstEntryBinary' AND resume == 'webhook' THEN SHOW parameter 'responseBinaryPropertyName'",
      "IF resume IN [\"webhook\", \"form\"] THEN SHOW parameter 'limitWaitTime'",
      "IF limitWaitTime == True AND resume IN [\"webhook\", \"form\"] THEN SHOW parameter 'limitType'",
      "IF limitType == 'afterTimeInterval' AND limitWaitTime == True AND resume IN [\"webhook\", \"form\"] THEN SHOW parameter 'resumeAmount'",
      "IF limitType == 'afterTimeInterval' AND limitWaitTime == True AND resume IN [\"webhook\", \"form\"] THEN SHOW parameter 'resumeUnit'",
      "IF limitType == 'atSpecifiedTime' AND limitWaitTime == True AND resume IN [\"webhook\", \"form\"] THEN SHOW parameter 'maxDateAndTime'",
      "IF resume == 'webhook' THEN SHOW parameter 'options'",
      "IF /httpMethod IN [\"PATCH\", \"PUT\", \"POST\"] AND @version == 1 THEN SHOW parameter 'binaryData'",
      "IF binaryData == True AND @version == 1 THEN SHOW parameter 'binaryPropertyName'",
      "IF @version == 1 THEN HIDE parameter 'binaryPropertyName'",
      "IF /responseMode == 'onReceived' THEN SHOW parameter 'noResponseBody'",
      "IF rawBody == True THEN HIDE parameter 'noResponseBody'",
      "IF @version == 1 THEN SHOW parameter 'rawBody'",
      "IF binaryData == True AND noResponseBody == True THEN HIDE parameter 'rawBody'",
      "IF noResponseBody == True AND @version == 1 THEN HIDE parameter 'rawBody'",
      "IF /responseMode == 'onReceived' THEN SHOW parameter 'responseData'",
      "IF noResponseBody == True THEN HIDE parameter 'responseData'",
      "IF /responseData == 'firstEntryJson' AND /responseMode == 'lastNode' THEN SHOW parameter 'responseContentType'",
      "IF /responseData == 'firstEntryJson' AND /responseMode == 'lastNode' THEN SHOW parameter 'responsePropertyName'",
      "IF resume == 'form' THEN SHOW parameter 'options'",
      "IF responseMode == 'responseNode' THEN HIDE parameter 'options'",
      "IF respondWith == 'text' THEN SHOW parameter 'formSubmittedText'",
      "IF respondWith == 'redirect' THEN SHOW parameter 'redirectUrl'",
      "IF responseMode IN [\"onReceived\", \"lastNode\"] THEN HIDE parameter 'options'"
    ]
  }
