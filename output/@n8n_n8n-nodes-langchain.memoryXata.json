{
  "node_id": "@n8n/n8n-nodes-langchain.memoryXata",
  "name": "@n8n/n8n-nodes-langchain.memoryXata",
  "version": [
    1,
    1.1,
    1.2,
    1.3,
    1.4
  ],
  "semantic_context": "Use Xata Memory",
  "parameters": {
    "notice": {
      "displayName": "This node must be connected to an AI agent. <a data-action='openSelectiveNodeCreator' data-action-parameter-creatorview='AI'>Insert one</a>",
      "name": "notice",
      "type": "notice",
      "default": "",
      "typeOptions": {
        "containerClass": "ndv-connection-hint-notice"
      }
    },
    "sessionId": {
      "displayName": "Session ID",
      "name": "sessionId",
      "type": "string",
      "default": "={{ $json.sessionId }}",
      "description": "The key to use to store the memory",
      "displayOptions": {
        "show": {
          "@version": [
            1.1
          ]
        }
      },
      "ai_hint": "The key to use to store the memory"
    },
    "sessionIdType": {
      "displayName": "Session ID",
      "name": "sessionIdType",
      "type": "options",
      "options": [
        {
          "name": "Connected Chat Trigger Node",
          "value": "fromInput",
          "description": "Looks for an input field called 'sessionId' that is coming from a directly connected Chat Trigger"
        },
        {
          "name": "Define below",
          "value": "customKey",
          "description": "Use an expression to reference data in previous nodes or enter static text"
        }
      ],
      "default": "fromInput",
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.2
              }
            }
          ]
        }
      },
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "sessionKey": {
      "displayName": "Session Key From Previous Node",
      "name": "sessionKey",
      "type": "string",
      "default": "={{ $json.sessionId }}",
      "disabledOptions": {
        "show": {
          "sessionIdType": [
            "fromInput"
          ]
        }
      },
      "displayOptions": {
        "show": {
          "sessionIdType": [
            "fromInput"
          ],
          "@version": [
            {
              "_cnd": {
                "gte": 1.4
              }
            }
          ]
        }
      }
    },
    "contextWindowLength": {
      "displayName": "Context Window Length",
      "name": "contextWindowLength",
      "type": "number",
      "default": 5,
      "hint": "How many past interactions the model receives as context",
      "displayOptions": {
        "hide": {
          "@version": [
            {
              "_cnd": {
                "lt": 1.3
              }
            }
          ]
        }
      }
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "notice",
      "sessionId",
      "sessionId",
      "sessionIdType",
      "sessionKey",
      "sessionKey",
      "contextWindowLength"
    ],
    "conditional_rules": [
      "IF @version == 1 THEN SHOW parameter 'sessionId'",
      "IF @version == 1.1 THEN SHOW parameter 'sessionId'",
      "IF @version == {'_cnd': {'gte': 1.2}} THEN SHOW parameter 'sessionIdType'",
      "IF sessionIdType == 'customKey' THEN SHOW parameter 'sessionKey'",
      "IF sessionIdType == 'fromInput' AND @version == {'_cnd': {'gte': 1.4}} THEN SHOW parameter 'sessionKey'",
      "IF @version == {'_cnd': {'lt': 1.3}} THEN HIDE parameter 'contextWindowLength'"
    ]
  }
}