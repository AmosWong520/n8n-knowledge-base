  "configuration_logic": {
    "required_sequence": [
      "descriptionType",
      "toolDescription",
      "authentication",
      "authentication",
      "resource",
      "operation",
      "driveId",
      "folderId",
      "title",
      "documentURL",
      "simple",
      "documentURL",
      "simple",
      "actionsUi",
      "updateFields"
    ],
    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF @version == 1 THEN SHOW parameter 'authentication'",
      "IF @version == 2 THEN SHOW parameter 'authentication'",
      "IF resource == 'document' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'document' THEN SHOW parameter 'driveId'",
      "IF operation == 'create' AND resource == 'document' THEN SHOW parameter 'folderId'",
      "IF operation == 'create' AND resource == 'document' THEN SHOW parameter 'title'",
      "IF operation == 'get' AND resource == 'document' THEN SHOW parameter 'documentURL'",
      "IF operation == 'get' AND resource == 'document' THEN SHOW parameter 'simple'",
      "IF operation == 'update' AND resource == 'document' THEN SHOW parameter 'documentURL'",
      "IF operation == 'update' AND resource == 'document' THEN SHOW parameter 'simple'",
      "IF operation == 'update' AND resource == 'document' THEN SHOW parameter 'actionsUi'",
      "IF object == 'text' THEN SHOW parameter 'action'",
      "IF object IN [\"footer\", \"header\", \"namedRange\", \"paragraphBullets\"] THEN SHOW parameter 'action'",
      "IF object IN [\"tableColumn\", \"tableRow\"] THEN SHOW parameter 'action'",
      "IF object IN [\"pageBreak\", \"table\"] THEN SHOW parameter 'action'",
      "IF object == 'positionedObject' THEN SHOW parameter 'action'",
      "IF object IN [\"footer\", \"header\", \"paragraphBullets\", \"namedRange\"] AND action == 'create' THEN SHOW parameter 'insertSegment'",
      "IF object IN [\"footer\", \"header\", \"paragraphBullets\", \"namedRange\"] AND action == 'create' THEN SHOW parameter 'segmentId'",
      "IF insertSegment == 'body' THEN HIDE parameter 'segmentId'",
      "IF object IN [\"footer\", \"header\"] AND action == 'create' THEN SHOW parameter 'index'",
      "IF object == 'namedRange' AND action == 'create' THEN SHOW parameter 'name'",
      "IF object == 'namedRange' AND action == 'create' THEN SHOW parameter 'startIndex'",
      "IF object == 'namedRange' AND action == 'create' THEN SHOW parameter 'endIndex'",
      "IF object == 'paragraphBullets' AND action == 'create' THEN SHOW parameter 'bulletPreset'",
      "IF object == 'footer' AND action == 'delete' THEN SHOW parameter 'footerId'",
      "IF object == 'header' AND action == 'delete' THEN SHOW parameter 'headerId'",
      "IF object == 'namedRange' AND action == 'delete' THEN SHOW parameter 'namedRangeReference'",
      "IF object == 'namedRange' AND action == 'delete' AND namedRangeReference == 'namedRangeId' THEN SHOW parameter 'value'",
      "IF object == 'namedRange' AND action == 'delete' AND namedRangeReference == 'name' THEN SHOW parameter 'value'",
      "IF object == 'positionedObject' AND action == 'delete' THEN SHOW parameter 'objectId'",
      "IF object IN [\"pageBreak\", \"table\", \"tableColumn\", \"tableRow\", \"text\"] AND action == 'insert' THEN SHOW parameter 'insertSegment'",
      "IF object IN [\"pageBreak\", \"table\", \"tableColumn\", \"tableRow\", \"text\"] AND action == 'insert' THEN SHOW parameter 'segmentId'",
      "IF object == 'pageBreak' AND action == 'insert' THEN SHOW parameter 'locationChoice'",
      "IF locationChoice == 'location' AND object == 'pageBreak' AND action == 'insert' THEN SHOW parameter 'index'",
      "IF object == 'table' AND action == 'insert' THEN SHOW parameter 'locationChoice'",
      "IF locationChoice == 'location' AND object == 'table' AND action == 'insert' THEN SHOW parameter 'index'",
      "IF object == 'table' AND action == 'insert' THEN SHOW parameter 'rows'",
      "IF object == 'table' AND action == 'insert' THEN SHOW parameter 'columns'",
      "IF object == 'text' AND action == 'insert' THEN SHOW parameter 'locationChoice'",
      "IF locationChoice == 'location' AND object == 'text' AND action == 'insert' THEN SHOW parameter 'index'",
      "IF object == 'text' AND action == 'insert' THEN SHOW parameter 'text'",
      "IF object == 'text' AND action == 'replaceAll' THEN SHOW parameter 'text'",
      "IF object == 'text' AND action == 'replaceAll' THEN SHOW parameter 'replaceText'",
      "IF object == 'text' AND action == 'replaceAll' THEN SHOW parameter 'matchCase'",
      "IF object IN [\"paragraphBullets\", \"tableColumn\", \"tableRow\"] AND action == 'delete' THEN SHOW parameter 'insertSegment'",
      "IF object IN [\"paragraphBullets\", \"tableColumn\", \"tableRow\"] AND action == 'delete' THEN SHOW parameter 'segmentId'",
      "IF object == 'paragraphBullets' THEN SHOW parameter 'startIndex'",
      "IF object == 'paragraphBullets' THEN SHOW parameter 'endIndex'",
      "IF object IN [\"tableColumn\", \"tableRow\"] AND action == 'insert' THEN SHOW parameter 'insertPosition'",
      "IF object IN [\"tableColumn\", \"tableRow\"] THEN SHOW parameter 'index'",
      "IF object IN [\"tableColumn\", \"tableRow\"] THEN SHOW parameter 'rowIndex'",
      "IF object IN [\"tableColumn\", \"tableRow\"] THEN SHOW parameter 'columnIndex'",
      "IF operation == 'update' AND resource == 'document' THEN SHOW parameter 'updateFields'"
    ]
  }
