  "configuration_logic": {
    "required_sequence": [
      "notice",
      "toolDescription",
      "method",
      "placeholderNotice",
      "url",
      "authentication",
      "nodeCredentialType",
      "googleApiWarning",
      "genericAuthType",
      "sendQuery",
      "specifyQuery",
      "parametersQuery",
      "jsonQuery",
      "sendHeaders",
      "specifyHeaders",
      "parametersHeaders",
      "jsonHeaders",
      "sendBody",
      "specifyBody",
      "parametersBody",
      "jsonBody",
      "placeholderDefinitions",
      "optimizeResponse",
      "responseType",
      "dataField",
      "fieldsToInclude",
      "fields",
      "cssSelector",
      "onlyContent",
      "elementsToOmit",
      "truncateResponse",
      "maxLength"
    ],
    "conditional_rules": [
      "IF authentication == 'predefinedCredentialType' THEN SHOW parameter 'nodeCredentialType'",
      "IF nodeCredentialType == 'googleApi' THEN SHOW parameter 'googleApiWarning'",
      "IF authentication == 'genericCredentialType' THEN SHOW parameter 'genericAuthType'",
      "IF sendQuery == True THEN SHOW parameter 'specifyQuery'",
      "IF sendQuery == True AND specifyQuery == 'keypair' THEN SHOW parameter 'parametersQuery'",
      "IF valueProvider == 'fieldValue' THEN SHOW parameter 'value'",
      "IF sendQuery == True AND specifyQuery == 'json' THEN SHOW parameter 'jsonQuery'",
      "IF sendHeaders == True THEN SHOW parameter 'specifyHeaders'",
      "IF sendHeaders == True AND specifyHeaders == 'keypair' THEN SHOW parameter 'parametersHeaders'",
      "IF sendHeaders == True AND specifyHeaders == 'json' THEN SHOW parameter 'jsonHeaders'",
      "IF sendBody == True THEN SHOW parameter 'specifyBody'",
      "IF sendBody == True AND specifyBody == 'keypair' THEN SHOW parameter 'parametersBody'",
      "IF sendBody == True AND specifyBody == 'json' THEN SHOW parameter 'jsonBody'",
      "IF optimizeResponse == True THEN SHOW parameter 'responseType'",
      "IF optimizeResponse == True AND responseType == 'json' THEN SHOW parameter 'dataField'",
      "IF optimizeResponse == True AND responseType == 'json' THEN SHOW parameter 'fieldsToInclude'",
      "IF optimizeResponse == True AND responseType == 'json' THEN SHOW parameter 'fields'",
      "IF fieldsToInclude == 'all' THEN HIDE parameter 'fields'",
      "IF optimizeResponse == True AND responseType == 'html' THEN SHOW parameter 'cssSelector'",
      "IF optimizeResponse == True AND responseType == 'html' THEN SHOW parameter 'onlyContent'",
      "IF optimizeResponse == True AND responseType == 'html' AND onlyContent == True THEN SHOW parameter 'elementsToOmit'",
      "IF optimizeResponse == True AND responseType IN [\"text\", \"html\"] THEN SHOW parameter 'truncateResponse'",
      "IF optimizeResponse == True AND responseType IN [\"text\", \"html\"] AND truncateResponse == True THEN SHOW parameter 'maxLength'"
    ]
  }
}