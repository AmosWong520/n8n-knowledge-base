      "IF resource == 'attribute' AND operation == 'create' AND attributeCategory == 'category' THEN SHOW parameter 'attributeCategoryList'",
      "IF resource == 'attribute' AND operation == 'update' THEN SHOW parameter 'updateAttributeCategory'",
      "IF resource == 'attribute' AND operation == 'update' THEN SHOW parameter 'updateAttributeName'",
      "IF resource == 'attribute' AND operation == 'update' THEN SHOW parameter 'updateAttributeValue'",
      "IF updateAttributeCategory == 'category' THEN HIDE parameter 'updateAttributeValue'",
      "IF resource == 'attribute' AND operation == 'update' AND updateAttributeCategory == 'category' THEN SHOW parameter 'updateAttributeCategoryList'",
      "IF resource == 'attribute' AND operation == 'delete' THEN SHOW parameter 'deleteAttributeCategory'",
      "IF resource == 'attribute' AND operation == 'delete' THEN SHOW parameter 'deleteAttributeName'",
      "IF resource == 'attribute' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'attribute' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
