      "IF operation == 'create' AND resource == 'opportunity' THEN SHOW parameter 'additionalFields'",
      "IF operation IN [\"get\", \"delete\"] AND resource == 'opportunity' THEN SHOW parameter 'opportunityId'",
      "IF resource == 'opportunity' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'opportunity' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation IN [\"getAll\", \"get\"] AND resource == 'opportunity' THEN SHOW parameter 'options'",
      "IF operation == 'update' AND resource == 'opportunity' THEN SHOW parameter 'opportunityId'",
      "IF operation == 'update' AND resource == 'opportunity' THEN SHOW parameter 'updateFields'",
      "IF resource == 'contact' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'contact' THEN SHOW parameter 'contactName'",
      "IF operation == 'create' AND resource == 'contact' THEN SHOW parameter 'additionalFields'",
      "IF operation IN [\"get\", \"delete\"] AND resource == 'contact' THEN SHOW parameter 'contactId'",
      "IF resource == 'contact' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'contact' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation IN [\"getAll\", \"get\"] AND resource == 'contact' THEN SHOW parameter 'options'",
      "IF operation == 'update' AND resource == 'contact' THEN SHOW parameter 'contactId'",
      "IF operation == 'update' AND resource == 'contact' THEN SHOW parameter 'updateFields'",
      "IF resource == 'note' THEN SHOW parameter 'operation'",
      "IF operation == 'create' AND resource == 'note' THEN SHOW parameter 'memo'",
