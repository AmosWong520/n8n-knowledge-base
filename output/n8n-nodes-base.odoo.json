      "IF operation IN [\"get\", \"delete\"] AND resource == 'custom' THEN SHOW parameter 'customResourceId'",
      "IF resource == 'custom' AND operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF resource == 'custom' AND operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation IN [\"getAll\", \"get\"] AND resource == 'custom' THEN SHOW parameter 'options'",
      "IF operation == 'getAll' AND resource == 'custom' THEN SHOW parameter 'filterRequest'",
      "IF operation == 'update' AND resource == 'custom' THEN SHOW parameter 'customResourceId'",
