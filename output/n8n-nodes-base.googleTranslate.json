{
  "node_id": "n8n-nodes-base.googleTranslate",
  "name": "n8n-nodes-base.googleTranslate",
  "version": [
    1,
    2
  ],
  "semantic_context": "Translate data using Google Translate",
  "parameters": {
    "authentication": {
      "displayName": "Authentication",
      "name": "authentication",
      "type": "options",
      "options": [
        {
          "name": "OAuth2 (recommended)",
          "value": "oAuth2"
        },
        {
          "name": "Service Account",
          "value": "serviceAccount"
        }
      ],
      "default": "oAuth2",
      "displayOptions": {
        "show": {
          "@version": [
            2
          ]
        }
      },
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。 与鉴权相关，注意不要在公共日志中输出敏感信息。"
    },
    "resource": {
      "displayName": "Resource",
      "name": "resource",
      "type": "options",
      "noDataExpression": true,
      "options": [
        {
          "name": "Language",
          "value": "language"
        },
        {
          "name": "Custom API Call",
          "value": "__CUSTOM_API_CALL__"
        }
      ],
      "default": "language",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "operation": {
      "displayName": "Operation",
      "name": "operation",
      "type": "options",
      "noDataExpression": true,
      "displayOptions": {
        "show": {
          "resource": [
            "language"
          ]
        }
      },
      "options": [
        {
          "name": "Translate",
          "value": "translate",
          "description": "Translate data",
          "action": "Translate a language"
        },
        {
          "name": "Custom API Call",
          "value": "__CUSTOM_API_CALL__"
        }
      ],
      "default": "translate",
      "ai_hint": "根据 REST 规范选择方法: 查询用 GET，创建用 POST，更新用 PUT/PATCH。"
    },
    "text": {
      "displayName": "Text",
      "name": "text",
      "type": "string",
      "default": "",
      "description": "The input text to translate",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "translate"
          ]
        }
      },
      "ai_hint": "The input text to translate"
    },
    "translateTo": {
      "displayName": "Translate To",
      "name": "translateTo",
      "type": "options",
      "typeOptions": {
        "loadOptionsMethod": "getLanguages"
      },
      "default": "",
      "description": "The language to use for translation of the input text, set to one of the language codes listed in <a href=\"https://cloud.google.com/translate/docs/languages\">Language Support</a>. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "translate"
          ]
        }
      },
      "ai_hint": "The language to use for translation of the input text, set to one of the language codes listed in <a href=\"https://cloud.google.com/translate/docs/languages\">Language Support</a>. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>."
    }
  },
  "configuration_logic": {
    "required_sequence": [
      "authentication",
      "authentication",
      "resource",
      "operation",
      "text",
      "translateTo"
    ],
    "conditional_rules": [
      "IF @version == 1 THEN SHOW parameter 'authentication'",
      "IF @version == 2 THEN SHOW parameter 'authentication'",
      "IF resource == 'language' THEN SHOW parameter 'operation'",
      "IF operation == 'translate' THEN SHOW parameter 'text'",
      "IF operation == 'translate' THEN SHOW parameter 'translateTo'"
    ]
  }
}