          "extractValue": {
            "type": "regex",
            "regex": ".*analytics\\.google\\.com\\/analytics.*\\/p([0-9]{1,})(?:\\/.*|)"
          }
        },
        {
          "displayName": "By ID",
          "name": "id",
          "type": "string",
          "placeholder": "123456",
          "validation": [
            {
              "type": "regex",
              "properties": {
                "regex": "[0-9]{1,}",
                "errorMessage": "Not a valid Google Analytics Property ID"
              }
            }
          ],
          "url": "=https://analytics.google.com/analytics/web/#/p{{$value}}/"
        }
