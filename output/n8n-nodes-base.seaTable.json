  "configuration_logic": {
    "required_sequence": [
      "resource",
      "operation",
      "tableName",
      "tableId",
      "rowId",
      "fieldsToSend",
      "inputsToIgnore",
      "columnsUi",
      "rowId",
      "rowId",
      "returnAll",
      "limit",
      "filters",
      "options"
    ],
    "conditional_rules": [
      "IF operation == 'get' THEN HIDE parameter 'tableName'",
      "IF operation == 'get' THEN SHOW parameter 'tableId'",
      "IF operation == 'update' THEN SHOW parameter 'rowId'",
      "IF operation IN [\"create\", \"update\"] THEN SHOW parameter 'fieldsToSend'",
      "IF operation IN [\"create\", \"update\"] AND fieldsToSend == 'autoMapInputData' THEN SHOW parameter 'inputsToIgnore'",
      "IF operation IN [\"create\", \"update\"] AND fieldsToSend == 'defineBelow' THEN SHOW parameter 'columnsUi'",
      "IF operation == 'delete' THEN SHOW parameter 'rowId'",
      "IF operation == 'get' THEN SHOW parameter 'rowId'",
      "IF operation == 'getAll' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation == 'getAll' THEN SHOW parameter 'filters'",
      "IF operation == 'getAll' THEN SHOW parameter 'options'"
    ]
