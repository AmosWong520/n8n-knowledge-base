  "configuration_logic": {
    "required_sequence": [
      "descriptionType",
      "toolDescription",
      "resource",
      "operation",
      "list",
      "email",
      "contactId",
      "resolveData",
      "additionalFields",
      "updateFields",
      "by",
      "contactId",
      "email",
      "returnAll",
      "limit",
      "simple"
    ],
    "conditional_rules": [
      "IF descriptionType == 'manual' THEN SHOW parameter 'toolDescription'",
      "IF operation IN [\"getAll\", \"create\", \"update\", \"get\"] THEN SHOW parameter 'list'",
      "IF operation == 'create' THEN SHOW parameter 'email'",
      "IF resource == 'contact' AND operation == 'update' THEN SHOW parameter 'contactId'",
      "IF operation IN [\"create\", \"update\"] THEN SHOW parameter 'resolveData'",
      "IF operation == 'create' AND resource == 'contact' THEN SHOW parameter 'additionalFields'",
      "IF operation == 'update' THEN SHOW parameter 'updateFields'",
      "IF operation == 'get' AND resource == 'contact' THEN SHOW parameter 'by'",
      "IF resource == 'contact' AND operation == 'get' AND by == 'id' THEN SHOW parameter 'contactId'",
      "IF resource == 'contact' AND operation == 'get' AND by == 'email' THEN SHOW parameter 'email'",
      "IF operation == 'getAll' AND resource == 'contact' THEN SHOW parameter 'returnAll'",
      "IF operation == 'getAll' AND resource == 'contact' AND returnAll == False THEN SHOW parameter 'limit'",
      "IF operation IN [\"get\", \"getAll\"] AND resource == 'contact' THEN SHOW parameter 'simple'"
    ]
  }
